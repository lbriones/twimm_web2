<%- model_class = Logtransfer -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:fromaccount) %></th>
      <th><%= model_class.human_attribute_name(:toaccount) %></th>
      <th><%= model_class.human_attribute_name(:monto) %></th>
      <th><%= model_class.human_attribute_name(:status) %></th>
      <th><%= model_class.human_attribute_name(:message) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @logtransfers.each do |logtransfer| %>
      <tr>
        <td><%= link_to logtransfer.id, logtransfer_path(logtransfer) %></td>
        <td><%= logtransfer.fromaccount %></td>
        <td><%= logtransfer.toaccount %></td>
        <td><%= logtransfer.monto %></td>
        <td><%= logtransfer.status %></td>
        <td><%= logtransfer.message %></td>
        <td><%=l logtransfer.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_logtransfer_path(logtransfer), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      logtransfer_path(logtransfer),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_logtransfer_path,
            :class => 'btn btn-primary' %>
