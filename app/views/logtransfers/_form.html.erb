<%= form_for @logtransfer, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :fromaccount, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :fromaccount, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :toaccount, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :toaccount, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :monto, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :monto, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :status, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :status, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :message, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :message, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                logtransfers_path, :class => 'btn' %>
  </div>
<% end %>
